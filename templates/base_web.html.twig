<!DOCTYPE html>
<html{{ component('html_element_attributes') }}>
	<head>
		<meta charset="{{ app.charset }}">
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
		<title>{% block title %}Website{% endblock %}</title>
		{% block favicon %}
		<link rel="icon" href="{% block favicon_url %}{{ asset(app.param('favicon.main', asset('favicons/favicon.svg'))) }}{% endblock %}">
		{% endblock %}
        {% block stylesheets %}
		{# <link rel="stylesheet" href="{{ asset('bundles/aequationwire/styles/wireadmin.css') }}"> #}
        {% endblock %}
		{% block javascripts %}
			{% block importmap %}{{ importmap('app') }}{% endblock %}
		{% endblock %}
		{{ app.getTurboMetas() }}
	</head>
	<body id="app">
		<div class="max-w-7xl px-4 mt-4 pb-24 mx-auto">
			{% block body %}
			<h5>Base web from Aequation Wire Bundle</h5>
			{% endblock %}
		</div>
	</body>
</html>
