{
    "name": "@aequation/wire",
    "description": "Utilities integration for Symfony",
    "license": "MIT",
    "version": "1.0.0",
    "keywords": [
        "symfony-ux"
    ],
    "type": "module",
    "symfony": {
        "controllers": {
            "globalapp": {
                "main": "dist/globalapp_controller.js",
                "fetch": "eager",
                "enabled": true
            },
            "scrollup": {
                "main": "dist/scrollup_controller.js",
                "fetch": "eager",
                "enabled": true
            },
            "toastr": {
                "main": "dist/toastr_controller.js",
                "fetch": "eager",
                "enabled": true,
                "autoimport": {
                    "toastr/build/toastr.min.css": true
                }
            }
        },
        "importmap": {
            "@hotwired/stimulus": "^3.2.2",
            "flowbite": "^3.1.2",
            "toastr": "^2.1.4",
            "browser": "^0.2.6"
        }
    },
    "peerDependencies": {
        "@hotwired/stimulus": "^3.2.2",
        "flowbite": "^3.1.2",
        "toastr": "^2.1.4",
        "browser": "^0.2.6"
    },
    "DevDependencies": {
        "@hotwired/stimulus": "^3.2.2",
        "flowbite": "^3.1.2",
        "toastr": "^2.1.4",
        "browser": "^0.2.6"
    }
}