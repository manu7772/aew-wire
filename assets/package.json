{
    "name": "@aequation/wire",
    "description": "DataTables integration for Symfony",
    "license": "MIT",
    "version": "1.0.0",
    "keywords": [
        "symfony-ux"
    ],
    "type": "module",
    "files": [
        "dist"
    ],
    "main": "dist/controller.js",
    "symfony": {
        "controllers": {
            "datatable": {
                "main": "dist/controller.js",
                "fetch": "eager",
                "webpackMode": "eager",
                "enabled": true,
                "autoimport": {
                    "simple-datatables/dist/style.min.css": true
                }
            }
        },
        "importmap": {
            "@hotwired/stimulus": "^3.2.2",
            "flowbite": "^3.1.2",
            "simple-datatables": "10.0.0"
        }
    },
    "peerDependencies": {
        "@hotwired/stimulus": "^3.2.2",
        "flowbite": "^3.1.2",
        "simple-datatables": "10.0.0"
    },
    "devDependencies": {
        "@hotwired/stimulus": "^3.2.2",
        "flowbite": "^3.1.2",
        "simple-datatables": "10.0.0"
    }
}